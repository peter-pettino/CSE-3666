
	.globl  main

        .text
	
main:	addi	s1, x0, 0	# s1 = 0
	addi	t1, x0, 31	# t1 = 31

loop:	srl	t0, s0, t1	# extract the most significant bit
	andi	t0, t0, 1	# AND the most significant bit and 1
	beq	t0, x0, skip	# if 0 then goto skip
	addi	s1, s1, 1	# s1 += 1

skip:	addi	t1, t1, -1	# t1 -= 1
	bge	t1, x0, loop	# if(t1>=0) goto loop
	

exit:   add	a0, x0, s1	# print s1
	addi	a7, x0, 1
	ecall
	
	addi    a7, x0, 10      
        ecall
